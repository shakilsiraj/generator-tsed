    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Generator",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/node_modules/.bin/yo",
            "args": [
                "tsed",
                "--skip-install"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Tests",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/node_modules/.bin/jest"
        }
    ]
}